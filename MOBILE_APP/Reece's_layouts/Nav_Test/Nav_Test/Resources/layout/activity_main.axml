<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:text="Make Booking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="500.0dp"
        android:layout_marginLeft="5.0dp"
        android:id="@+id/button_makeBooking"
        android:textColor="#ffffff"
        android:background="#00BFFF" />
    <Button
        android:text="View Tickets"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="500.0dp"
        android:layout_marginLeft="125.0dp"
        android:id="@+id/button_viewTickets"
        android:textColor="#ffffff"
        android:background="#00BFFF" />
    <Button
        android:text="View Expired Tickets"
        android:layout_width="120.0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="500.0dp"
        android:layout_marginLeft="230.0dp"
        android:id="@+id/button_viewTickets"
        android:textColor="#ffffff"
        android:background="#00BFFF" />
</RelativeLayout>